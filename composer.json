{
  "name": "carloschininin/data",
  "description": "Utilities for data",
  "type": "library",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Carlos Chininin",
      "email": "carloschininin@gmail.com"
    }
  ],
  "require": {
    "php": "^8.4",
    "carloschininin/util": "^0.2",
    "phpoffice/phpspreadsheet": "^4.0|^5.0",
    "symfony/http-foundation": "^6.0|^7.0|^8.0"
  },
  "require-dev": {
    "laravel/pint": "^1.22",
    "phpunit/phpunit": "^11.0|^12.0"
  },
  "suggest": {
    "ext-xlswriter": "Extension required xls fast",
    "mpdf/mpdf": "Option for rendering PDF with PDF Writer",
    "dompdf/dompdf": "Option for rendering PDF with PDF Writer (doesn't yet support PHP8)",
    "tecnickcom/tcpdf": "Option for rendering PDF with PDF Writer (doesn't yet support PHP8)",
    "jpgraph/jpgraph": "Option for rendering charts, or including charts with PDF or HTML Writers"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "CarlosChininin\\Data\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CarlosChininin\\Data\\Tests\\": "tests/"
    }
  }
}
